{
  "name": "mdcnt-css",
  "version": "0.0.1",
  "description": "wordpress-theme",
  "main": "index.js",
  "scripts": {
    "watch:build": "onchange 'src/**/*.sass' -- npm run build:css",
    "build:css": "npm-run-all clean-css kabuto:*",
    "kabuto:sass": "node-sass --output-style expanded -o css src/style/shard5.sass ",
    "kabuto:postcss": "postcss -c postcss.config.js -r css/shard5.css",
    "clean-css": "rimraf css/*.css",
    "cp-bulma": "cpx node_modules/bulma/sass/*.sass src/style",
    "cp-fa5": "cpx node_modules/@fortawesome/fontawesome-free/js/all.min.js css/js/fa5.js",
    "postinstall": "npm run kabuto & npm run watch",
    "autoprefix": "postcss -u autoprefixer --no-map --autoprefixer browsers 'last 2 versions' -r css/**/*.css",
    "serve": "browser-sync start --server --serveStatic css --files 'css/*.html, css/**/*.css' ",
    "pug:test": "pug -P src/test.pug --out css/",
    "pug:aP": "pug -P src/pug/ArchivesPage/index.pug --out css/",
    "pug:bP": "pug -P src/pug/BlogPost/index.pug --out css/",
    "pug:tP": "pug -P src/pug/TopPage/index.pug --out css/",
    "pug:404": "pug -P src/pug/NotFound404/index.pug --out css/",
    "pug:sP": "pug -P src/pug/SearchPage/index.pug --out css/",
    "pug:pP": "pug -P src/pug/PagePage/index.pug --out css/",
    "build:aP": "onchange src -- npm-run-all -p kabuto:sass autoprefix pug:aP",
    "build:bP": "onchange src -- npm-run-all -p kabuto:sass autoprefix pug:bP",
    "build:404": "onchange src -- npm-run-all -p kabuto:sass autoprefix pug:404",
    "build:tP": "onchange src -- npm-run-all -p kabuto:sass autoprefix pug:tP",
    "build:sP": "onchange src -- npm-run-all -p kabuto:sass autoprefix pug:sP",
    "build:pP": "onchange src -- npm-run-all -p kabuto:sass autoprefix pug:pP",
    "start:aP": "npm-run-all -p build:aP serve",
    "start:bP": "npm-run-all -p build:bP serve",
    "start:404": "npm-run-all -p build:404 serve",
    "start:sP": "npm-run-all -p build:sP serve",
    "start:pP": "npm-run-all -p build:pP serve",
    "start:tP": "npm-run-all -p build:tP serve"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "bootstrap": "^4.1.3",
    "browser-sync": "^2.24.7",
    "bulma": "^0.7.1",
    "cpx": "^1.5.0",
    "css-declaration-sorter": "^4.0.1",
    "csswring": "^7.0.0",
    "debug": "^3.1.0",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.0.0",
    "parallelshell": "^3.0.2",
    "postcss-cli": "^5.0.0",
    "postcss-extend": "^1.0.5",
    "postcss-for": "^2.1.1",
    "postcss-import": "^11.1.0",
    "postcss-nested": "^3.0.0",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.6.2",
    "sanitize.css": "^7.0.3",
    "sass-lint": "^1.12.1",
    "stylelint": "^9.6.0",
    "stylelint-config-primer": "^2.2.10"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yugioh-hack/xeory-mdcnt.git"
  },
  "keywords": [
    "wordpress"
  ],
  "author": "Yugioh Hack",
  "license": "GDP",
  "homepage": "https://github.com/yugioh-hack/xeory-mdcnt"
}
